# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development
LOG_LEVEL=info

# JWT Authentication Configuration
JWT_ISSUER=https://mock-fdx-auth.example.com
JWT_AUDIENCE=fdx-resource-api
JWKS_URI=https://mock-fdx-auth.example.com/.well-known/jwks.json

# Aggregator Configuration
# Supported providers: mock, yodlee
AGGREGATOR_PROVIDER=mock

# Yodlee Configuration (required when AGGREGATOR_PROVIDER=yodlee)
YODLEE_BASE_URL=https://sandbox.api.yodlee.com
YODLEE_CLIENT_ID=your_yodlee_client_id
YODLEE_CLIENT_SECRET=your_yodlee_client_secret